updated_at "timestamp" [Note: "เวลาที่อัปเดต record ล่าสุด"]
updated_by "bigint" [Note: "User ที่อัปเดต record ล่าสุด"]
deleted_at "timestamp" [Note: "เวลาที่ลบ record ล่าสุด"]
deleted_by "bigint" [Note: "User ที่ลบ record"]
}
